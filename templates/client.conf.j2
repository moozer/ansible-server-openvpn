
client
dev tun
proto udp

remote {{ openvpn_server_connect_name }} {{ openvpn_port }}
;remote my-server-2 1194

resolv-retry infinite
nobind

persist-key
persist-tun

ca ca.crt
cert client.crt
key client.key

remote-cert-tls server

tls-auth ta.key 1

cipher AES-256-CBC
auth SHA512

comp-lzo

;log         openvpn.log
log-append  {{ openvpn_log_dir }}/{{ openvpn_log_file }}
status {{ openvpn_log_dir }}/{{ openvpn_status_log_file }}
verb 3
